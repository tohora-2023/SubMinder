{
  "name": "boilerplate-fullstack-scss",
  "version": "2.0.0",
  "main": "server/index.js",
  "license": "MIT",
  "keywords": [
    "fullstack"
  ],
  "scripts": {
    "start": "ts-node server/index.ts",
    "test": "jest",
    "test:watch": "jest --noStackTrace --watchAll",
    "dev": "run-p dev:css dev:client dev:server",
    "dev:css": "npx tailwindcss -i ./client/styles/index.css -o ./server/public/styles.css --watch",
    "dev:client": "npm run webpack -- --watch",
    "dev:server": "nodemon server/index.ts",
    "build": "run-p build:client build:server",
    "build:client": "npm run webpack -- --mode production",
    "build:server": "npm run knex -- migrate:latest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "webpack": "webpack --config ./client/webpack.config.js",
    "knex": "knex --knexfile ./server/db/knexfile.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "@devacademy/eslint-config/react",
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "dependencies": {
    "@auth0/auth0-react": "^1.12.0",
    "@redux-devtools/extension": "^3.2.3",
    "@toast-ui/react-calendar": "^2.1.3",
    "auth0": "^3.3.0",
    "express": "^4.17.2",
    "express-jwt": "^8.4.1",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.1.5",
    "knex": "^2.3.0",
    "pg": "^8.8.0",
    "react-google-charts": "^4.0.0",
    "sqlite3": "^5.1.2",
    "superagent": "7.1.1",
    "tui-date-picker": "^4.3.3",
    "tui-time-picker": "^2.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.15.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.17.12",
    "@devacademy/eslint-config": "^1.8.0",
    "@testing-library/react": "^13.4",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-modal": "^3.13.1",
    "@types/superagent": "^4.1.16",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "eslint": "^8.10.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.3.1",
    "mini-css-extract-plugin": "^2.7.5",
    "nock": "^13.3.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.1.0",
    "postcss-preset-env": "^8.0.1",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "react": "^18.0.2",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "^6.4.2",
    "redux": "^4.1.1",
    "redux-thunk": "^2.4.2",
    "style-loader": "^3.3.0",
    "supertest": "^6.2.2",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.4",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "browserslist": "> 2%, not dead"
}
